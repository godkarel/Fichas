	<script>
		local function RetiraHP()
			if sheet.TipoRecebido == '% Fisico' then
				sheet.AplicaDefPorCent = (tonumber(sheet.HPRetirado) or 0) * (tonumber(sheet.DefesaPorCent) / 100);
			else
